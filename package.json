{
  "author": "Volkov Labs",
  "dependencies": {
    "@emotion/css": "^11.11.2",
    "@grafana/data": "10.1.5",
    "@grafana/runtime": "10.1.5",
    "@grafana/ui": "10.1.5",
    "file-saver": "^2.0.5",
    "js-base64": "^3.7.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-medium-image-zoom": "^5.1.8",
    "react-zoom-pan-pinch": "^3.3.0",
    "tslib": "^2.6.2"
  },
  "description": "Base64 Image/PDF panel",
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@grafana/e2e": "10.1.5",
    "@grafana/e2e-selectors": "10.1.5",
    "@grafana/eslint-config": "^6.0.1",
    "@grafana/tsconfig": "^1.3.0-rc1",
    "@swc/core": "^1.3.95",
    "@swc/helpers": "^0.5.3",
    "@swc/jest": "^0.2.29",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@types/file-saver": "^2.0.6",
    "@types/jest": "^29.5.6",
    "@types/lodash": "^4.14.200",
    "@types/node": "^18.18.7",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^9.0.1",
    "glob": "^10.3.10",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.0.3",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "Apache-2.0",
  "name": "volkovlabs-image-panel",
  "scripts": {
    "build": "webpack -c ./webpack.config.ts --env production",
    "dev": "webpack -w -c ./webpack.config.ts --env development",
    "e2e": "npm exec cypress install && npm exec grafana-e2e run",
    "e2e:update": "npm exec cypress install && npm exec grafana-e2e run --update-screenshots",
    "levitate": "npx @grafana/levitate@latest is-compatible --path src/module.ts --target @grafana/data,@grafana/runtime,@grafana/ui",
    "lint": "eslint --ignore-path ./.eslintignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "eslint --fix --ignore-path ./.eslintignore --ext .js,.jsx,.ts,.tsx .",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "start": "docker-compose pull && docker-compose up",
    "stop": "docker-compose down",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --maxWorkers 4 --coverage",
    "upgrade": "npm upgrade --save"
  },
  "version": "4.1.0"
}
